/* top_keywords.jsp */
#header-top {
	position: relative;
	background-color: #f1f3f5;
	color: #212529
}

#header-top #header-title-wrap {
	position: relative;
	width: 980px;
	margin: 0 auto;
	padding-top: 200px;
	padding-bottom: 431px
}

#header-top #header-title-wrap #header-title {
	text-align: center;
	font-size: 40px;
	font-weight: 600;
	line-height: 1.35;
	letter-spacing: -0.6px
}

#header-top #header-title-wrap #header-title-desc {
	text-align: center;
	margin-top: 8px;
	font-size: 17px;
	line-height: 1.6;
	letter-spacing: -0.6px
}

#header-top #header-title-wrap #header-image {
	position: absolute;
	bottom: 0;
	left: 0;
	background-image:
		url(https://d3qlrgda07sb6k.cloudfront.net/assets/home/base/phone-ee2960c1cbf1e932ac291ad59eacc55ef6dc3cab3e9e4ea4284c4e0256bb9f4c.png);
	background-size: 760px 396px;
	width: 760px;
	height: 396px
}

#header-top #top-keywords {
	position: absolute;
	top: 280px;
	left: 770px;
	color: #212529
}

#header-top #top-keywords #top-keywords-title {
	margin-bottom: 24px
}

#header-top #top-keywords #top-keywords-title #top-keywords-link {
	display: block;
	font-size: 17px;
	font-weight: bold;
	text-align: left;
	color: #212529;
	text-decoration: none;
	padding: 16px 0;
	border-bottom: 1px solid #868e96
}

#header-top #top-keywords #top-keywords-title #top-keywords-link #top-keywords-title-text
	{
	margin-right: 66px
}

#header-top #top-keywords #top-keywords-title #top-keywords-link .right-arrow
	{
	stroke: #212529
}

#header-top #top-keywords #top-keywords-title #top-keywords-link:hover {
	color: #f76707
}

#header-top #top-keywords #top-keywords-title #top-keywords-link:hover .right-arrow
	{
	stroke: #f76707
}

#header-top #top-keywords #top-keywords-list {
	margin: 24px 0;
	list-style-type: none;
	color: #212529
}

#header-top #top-keywords #top-keywords-list li {
	display: block;
	counter-increment: item;
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	font-size: 17px;
	font-weight: normal;
	line-height: 1;
	margin: 20px 0;
	position: relative
}

#header-top #top-keywords #top-keywords-list li a {
	color: #212529;
	text-decoration: none;
	-webkit-box-flex: 1;
	-ms-flex: 1;
	flex: 1;
	display: block
}

#header-top #top-keywords #top-keywords-list li a .keyword-text {
	display: inline-block
}

#header-top #top-keywords #top-keywords-list li a .rank {
	-webkit-box-flex: 0;
	-ms-flex: none;
	flex: none;
	position: absolute;
	right: 8px;
	top: 0;
	line-height: 1
}

#header-top #top-keywords #top-keywords-list li a .rank .new {
	color: #e03131;
	display: inline-block;
	vertical-align: middle;
	font-size: 13px
}

#header-top #top-keywords #top-keywords-list li a .rank .up {
	color: #e03131;
	font-size: 10px;
	vertical-align: middle
}

#header-top #top-keywords #top-keywords-list li a .rank .down {
	color: #186ec0;
	font-size: 10px;
	vertical-align: middle
}

#header-top #top-keywords #top-keywords-list li a .rank .changed_rank {
	display: inline-block;
	vertical-align: middle;
	font-size: 13px
}

#header-top #top-keywords #top-keywords-list li:before {
	content: counter(item);
	display: inline-block;
	line-height: 1;
	font-size: 17px;
	font-weight: bold;
	margin-right: 8px
}

#header-top #top-keywords #top-keywords-list li:hover a {
	color: #f76707
}

#header-top #top-keywords #top-keywords-list li:hover a .rank .new,
	#header-top #top-keywords #top-keywords-list li:hover a .rank .up,
	#header-top #top-keywords #top-keywords-list li:hover a .rank .down {
	color: #f76707
}

#header-top #header-title-wrap #header-image {
	background-image:
		url(https://d3qlrgda07sb6k.cloudfront.net/assets/home/base/phone@2x-9611a35b152568b8a6c1deaff4b665668e3350c62382eccfaf496e974cef5d50.png)
}

/* hot articles.jsp */
.cards-wrap {
	margin: 0 auto;
	text-align: center
} /* css가 먹히지 않음 ㅠ */
.cards-wrap .card-link {
	text-decoration: none;
	color: #212529
}

.cards-wrap .card-top, .cards-wrap .card {
	position: relative;
	text-align: left;
	display: inline-block;
	border-radius: 8px;
	border: 1px solid #e9ecef;
	width: calc(25% - 34px);
	margin-right: 34px;
	margin-bottom: 40px
}

.cards-wrap .card-top .card-photo, .cards-wrap .card .card-photo {
	width: 100%;
	position: relative;
	height: 160px;
	overflow: hidden;
	border-radius: 8px 8px 0 0;
	background-color: #f8f9fa
}

.cards-wrap .card-top .card-photo img, .cards-wrap .card .card-photo img
	{
	width: 100%;
	position: relative;
	left: 50%;
	-webkit-transform: translate(-50%, 0);
	transform: translate(-50%, 0)
}

.cards-wrap .card-top .card-photo .category-image, .cards-wrap .card .card-photo .category-image
	{
	width: 100px;
	height: 100px;
	-webkit-transform: translate(-50%, 30%);
	transform: translate(-50%, 30%)
}

.cards-wrap .card-top .card-desc, .cards-wrap .card .card-desc {
	padding: 16px;
	padding-bottom: 0
}

.cards-wrap .card-top .card-desc .card-title, .cards-wrap .card .card-desc .card-title
	{
	font-size: 17px;
	font-weight: 600;
	letter-spacing: -0.6px;
	color: #212529;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	margin-bottom: 6px;
	line-height: 1.2
}

.cards-wrap .card-top .card-desc .card-region-name, .cards-wrap .card .card-desc .card-region-name
	{
	font-size: 15px;
	color: #868e96;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	margin-bottom: 8px;
	line-height: 1.3
}

.cards-wrap .card-top .card-desc .card-price, .cards-wrap .card .card-desc .card-price
	{
	font-size: 15px;
	font-weight: 600;
	padding-bottom: 8px;
	border-bottom: 1px solid #e9ecef;
	line-height: 1.3;
	color: #f76707
}

.cards-wrap .card-top .card-desc .card-price.blank-price, .cards-wrap .card .card-desc .card-price.blank-price
	{
	color: #212529
}

.cards-wrap .card-top .card-desc .card-counts, .cards-wrap .card .card-desc .card-counts
	{
	padding: 12px 0;
	color: #adb5bd;
	font-size: 13px
}

.cards-wrap .card {
	width: calc(33% - 14px);
	margin-right: 16px
}

.cards-wrap .card-top:hover, .cards-wrap .card:hover {
	box-shadow: 0 6px 10px 0 rgba(0, 0, 0, 0.2);
	-webkit-box-shadow: 0 6px 10px 0 rgba(0, 0, 0, 0.2);
	-moz-box-shadow: 0 6px 10px 0 rgba(0, 0, 0, 0.2);
	top: -2px
}

.cards-wrap .card-top:hover .home-icon-add, .cards-wrap .card:hover .home-icon-add
	{
	background-position: -123px -1380px
}

.cards-wrap .card-top:nth-child(4n), .cards-wrap .card:nth-child(3n) {
	margin-right: 0
}

.cards-wrap #cards-more {
	text-align: center;
	vertical-align: top
}

.cards-wrap #cards-more #cards-more-text {
	font-size: 17px;
	font-weight: 600;
	line-height: 1.5;
	letter-spacing: -0.6px;
	padding: 88px 0
}

.cards-wrap #cards-more #cards-more-text #more-text {
	padding-top: 15px
}

.title-line-divider {
	width: 70px;
	border-top: 2px solid #f76707;
	margin: 0 auto;
	margin-bottom: 50px
}

#hot-articles {
	position: relative;
	z-index: 1;
	background-color: #fff;
	padding: 120px 0
}

#hot-articles .cards-wrap {
	width: 980px;
    margin: 0 auto;
    text-align: left;
}

#hot-articles #hot-articles-nanum-wrap {
	position: relative;
	width: 980px;
	margin: 0 auto
}

#hot-articles #hot-articles-nanum-wrap .cards-wrap {
	text-align: left;
	margin-left: 4px
}

#open-regions {
	padding: 110px 0
}

#open-regions #open-regions-list {
	width: 980px;
	margin: 0 auto;
	list-style-type: none;
	text-align: center
}

#open-regions #open-regions-list li {
	display: inline-block;
	margin: 8px;
	margin-top: 0;
	border-radius: 100px;
	border: solid 1px #e9ecef;
	padding: 10px 20px
}

#open-regions #open-regions-list li a {
	font-size: 17px;
	letter-spacing: -0.6px;
	text-decoration: none;
	color: #212529
}

#open-regions #open-regions-list li:hover a {
	color: #868e96
}

/* introduct.jsp */
#introduction {
	text-align: center;
	padding-top: 120px;
	padding-bottom: 20px;
	background-color: #f8f9fa
}

#introduction #introduction-wrap {
	width: 980px;
	margin: 0 auto
}

#introduction .introduction-detail {
	display: inline-block;
	margin-right: 138px;
	margin-bottom: 100px
}

#introduction .introduction-detail .introduction-title {
	margin-top: 32px;
	font-size: 22px;
	font-weight: 600;
	line-height: 1.6;
	letter-spacing: -0.6px;
	text-align: center
}

#introduction .introduction-detail .introduction-desc {
	margin-top: 6px;
	font-size: 17px;
	line-height: 1.59;
	letter-spacing: -1.2px;
	text-align: center;
	color: #495057
}

#introduction .introduction-detail:nth-child(3n) {
	margin-right: 0
}

.home-chatting, .home-checkin, .home-notification, .home-share,
	.home-temperature, .home-thanks, .home-logo, .home-icon-add:hover,
	.home-icon-add, .home-icon-quotation {
	display: inline-block;
	background:
		url(https://d3qlrgda07sb6k.cloudfront.net/assets/home/base/icon-set-50e8f0ea464db3f581f26893911142973cff6fedc7c2c917725338ddd47f5648.png)
		no-repeat;
	overflow: hidden;
	text-indent: -9999px;
	text-align: left
}

@media only screen and (-webkit-min-device-pixel-ratio: 1.5) , only
	screen and (min-resolution: 1.5dppx) {
	.home-chatting, .home-checkin, .home-notification, .home-share,
		.home-temperature, .home-thanks, .home-logo, .home-icon-add:hover,
		.home-icon-add, .home-icon-quotation {
		background:
			url(https://d3qlrgda07sb6k.cloudfront.net/assets/home/base/icon-set@2x-f3a71226a57dc10686a03d9336d94ae48731c6400f91d585711e3cb0ea553c78.png)
			no-repeat;
		background-size: 230px 1417px
	}
	#header-top #header-title-wrap #header-image {
		background-image:
			url(https://d3qlrgda07sb6k.cloudfront.net/assets/home/base/phone@2x-9611a35b152568b8a6c1deaff4b665668e3350c62382eccfaf496e974cef5d50.png)
	}
}

