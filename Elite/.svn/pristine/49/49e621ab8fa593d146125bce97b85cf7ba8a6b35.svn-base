	margin-bottom: 50px;
	border-bottom: 1px solid #cccccc;
}

div.title_div h2.title_h2 {
	font-size: 40px;
	color: #4d4d4d;
	letter-spacing: -0.6px;
	padding-top: 35px;
}

div.line_member {
	color: white;
	margin-left: -180px;
	width: 1050px;
	margin-bottom: 50px;
}

div.title_div h2.title_h2 span, div.title_div h2.title_h2 a {
