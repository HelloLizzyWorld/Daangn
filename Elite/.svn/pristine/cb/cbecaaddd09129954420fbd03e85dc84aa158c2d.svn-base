.cards-wrap{margin:0 auto;text-align:center}
.cards-wrap .card-link{text-decoration:none;color:#212529}
.cards-wrap .card-top,.cards-wrap .card{position:relative;text-align:left;display:inline-block;border-radius:8px;border:1px solid #e9ecef;width:calc(25% - 34px);/* margin-right:34px; */margin-bottom:40px}
.cards-wrap .card-top .card-photo,.cards-wrap .card .card-photo{width:100%;position:relative;height:160px;overflow:hidden;border-radius:8px 8px 0 0;background-color:#f8f9fa}
