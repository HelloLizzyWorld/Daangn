.cards-wrap .card-top .card-desc .card-title,.cards-wrap .card .card-desc .card-title{font-size:17px;font-weight:600;letter-spacing:-0.6px;color:#212529;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;margin-bottom:6px;line-height:1.2}
.cards-wrap .card-top .card-desc .card-region-name,.cards-wrap .card .card-desc .card-region-name{font-size:15px;color:#868e96;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;margin-bottom:8px;line-height:1.3}
.cards-wrap .card-top .card-desc .card-price,.cards-wrap .card .card-desc .card-price{font-size:15px;font-weight:600;padding-bottom:8px;border-bottom:1px solid #e9ecef;line-height:1.3;color:#f76707}
.cards-wrap .card-top .card-desc .card-price.blank-price,.cards-wrap .card .card-desc .card-price.blank-price{color:#212529}.cards-wrap .card-top .card-desc .card-counts,.cards-wrap .card .card-desc .card-counts{padding:12px 0;color:#adb5bd;font-size:13px}
.cards-wrap .card{width:calc(33% - 14px);/* margin-right:16px */}
.cards-wrap .card-top:hover,.cards-wrap .card:hover{box-shadow:0 6px 10px 0 rgba(0,0,0,0.2);-webkit-box-shadow:0 6px 10px 0 rgba(0,0,0,0.2);-moz-box-shadow:0 6px 10px 0 rgba(0,0,0,0.2);top:-2px}
.cards-wrap .card-top:hover .home-icon-add,.cards-wrap .card:hover .home-icon-add{background-position:-123px -1380px}
.cards-wrap .card-top:nth-child(4n),.cards-wrap .card:nth-child(3n){margin-right:0}
.cards-wrap #cards-more{text-align:center;vertical-align:top}
.cards-wrap #cards-more #cards-more-text{font-size:17px;font-weight:600;line-height:1.5;letter-spacing:-0.6px;padding:88px 0}
.cards-wrap #cards-more #cards-more-text #more-text{padding-top:15px}
