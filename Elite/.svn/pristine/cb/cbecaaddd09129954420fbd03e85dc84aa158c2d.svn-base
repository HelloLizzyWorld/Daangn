#fixed-bar #fixed-bar-wrap #fixed-bar-download .fixed-download-button .fixed-download-text{display:inline-block;color:#f76707;font-size:17px;margin-left:4px;vertical-align:middle}
#fixed-bar #fixed-bar-wrap #fixed-bar-download .fixed-download-button .fixed-google-play,#fixed-bar #fixed-bar-wrap #fixed-bar-download .fixed-download-button .fixed-apple-store{fill:#f76707;vertical-align:middle}
