/* 상단 선택창 */
 .page-tabs{
	border-top:none;
	height:800px;
}

.tabs {
	display: table;
	width: 100%;
	height: 50px;
	table-layout: fixed;
	margin-bottom: 20px;
	
}

.tab {
	display: table-cell;
	background: #fafafa;
	vertical-align: middle;
	text-align: center;
	font-weight: 500;
	font-size: 15px;
	color: gray;
	border-top: 1px solid #ccc;
	border-right: 1px solid #ccc;
	border-bottom: 1px solid #212121
}

.tab:first-child {
	border-left: 1px solid #ccc
}

.tab:first-child.active{
	border-left: 1px solid #212121;
}

.tab.active {
	background: #fff;
	color: #212121;
	border-bottom: 0;
	border-top: 1px solid #212121;
	font-weight: 700;
	
}

.tab.active, .tab.support{
	border-right: 1px solid #212121
}




